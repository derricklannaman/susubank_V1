<!DOCTYPE html>
<html>
<head>
  <title>Susubank</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
 <div class="container">
  <%= render "shared/nav" %>

  <% if flash[:notice] %>
    <div class="notice"><%= flash[:notice] %></div>
  <% end %>

  <div  class="wrapper" id="content">
   <%= yield %>
  </div>

  <%= render "shared/footer" %>
 </div>
</body>
</html>
